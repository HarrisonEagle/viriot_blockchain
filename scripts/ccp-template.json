{
    "name": "viriot-network-org${ORG}",
    "version": "1.0.0",
    "client": {
        "organization": "Org${ORG}",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "channels": {
        "mychannel": {
          "orderers": [
            "org0-orderer${ORG}"
          ],
          "peers": {
            "org${ORG}-peers": {
              "endorsingPeer": true,
              "chaincodeQuery": true,
              "ledgerQuery": true,
              "eventSource": true
            }
          }
        }
    },
    "organizations": {
        "Org${ORG}": {
            "mspid": "Org${ORG}MSP",
            "peers": [
                "org${ORG}-peers"
            ],
            "orderers": [
                "org0-orderer${ORG}"
            ],
            "certificateAuthorities": [
                "org${ORG}-ca"
            ]
        }
    },
    "peers": {
        "org${ORG}-peers": {
            "url": "grpcs://org${ORG}-peer-gateway-svc:7051",
            "tlsCACerts": {
                "pem": "${PEERPEM}"
            },
            "grpcOptions": {
                "ssl-target-name-override": "org${ORG}-peer-gateway-svc",
                "hostnameOverride": "org${ORG}-peer-gateway-svc"
            }
        }
    },
    "orderers": {
        "org0-orderer${ORG}": {
          "url": "grpcs://org0-orderer${ORG}:6050",
          "grpcOptions": {
            "ssl-target-name-override": "org0-orderer${ORG}",
            "hostnameOverride": "org0-orderer${ORG}"
          },
          "tlsCACerts": {
            "pem": "${ORDERPEM}"
          }
        }
    },
    "certificateAuthorities": {
        "org${ORG}-ca": {
            "url": "https://org${ORG}-ca:443",
            "caName": "org${ORG}-ca",
            "tlsCACerts": {
                "pem": ["${CAPEM}"]
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
